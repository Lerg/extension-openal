[project]
title = Defold OpenAL
version = 0.1
custom_resources = sounds/

[bootstrap]
main_collection = /main/main.collectionc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1280
height = 720

[physics]
scale = 0.02
gravity_y = 0

[script]
shared_state = 1

[library]
include_dirs = openal

[native_extension]
app_manifest = /openal/manifests/sound_null.appmanifest

[ios]
bundle_identifier = com.spiralcodestudio.test1

